/* 一行三列布局左图标中文字右图标 */
.wx-cells{
  width:100%;
  margin-top: 15rpx;
  font-size: 34rpx;
  border-top: 1rpx solid gainsboro;
  border-bottom: 1px solid gainsboro;
  background-color: white
}
.wx-cell{
  display: flex;
  padding: 20rpx 25rpx;
  align-items: center;
}
.wx-cell-icon{
  width: 50px;
  height: 56rpx;
}
.wx-cell-text{
flex: 1;
margin-left: 30rpx;
}
.wx-cell-arrow{
 width: 30rpx;
 height: 30rpx;
 margin-right: 10rpx
}